# AutoTaskTracker Environment Configuration
# Copy this file to .env and customize for your environment

# Database Configuration
AUTOTASK_DATABASE_URL=postgresql://postgres:mysecretpassword@localhost:5433/autotasktracker

# Pensieve Configuration  
PENSIEVE_CONFIG_SYNC=true
PENSIEVE_CACHE_ENABLED=true

# VLM Configuration
VLM_MODEL=llava:13b
VLM_PORT=11434

# Development Mode
AUTOTASK_DEBUG=false
AUTOTASK_LOG_LEVEL=INFO

# Virtual Environment
# Always use the project virtual environment: source venv/bin/activate